<svg width="1920" height="1080" viewBox="0 0 1920 1080" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="servicesGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#e2e8f0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#cbd5e1;stop-opacity:1" />
    </linearGradient>
    <pattern id="dots" width="30" height="30" patternUnits="userSpaceOnUse">
      <circle cx="15" cy="15" r="1.5" fill="rgba(59,130,246,0.1)"/>
    </pattern>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#servicesGradient)"/>
  
  <!-- Dot pattern -->
  <rect width="100%" height="100%" fill="url(#dots)"/>
  
  <!-- Service icons representation -->
  <g opacity="0.1">
    <!-- Web design icon -->
    <rect x="200" y="200" width="120" height="80" rx="8" fill="#3b82f6" stroke="#1e40af" stroke-width="2"/>
    <rect x="220" y="220" width="80" height="40" rx="4" fill="#ffffff"/>
    
    <!-- Mobile icon -->
    <rect x="500" y="180" width="60" height="120" rx="12" fill="#3b82f6" stroke="#1e40af" stroke-width="2"/>
    <rect x="515" y="200" width="30" height="80" rx="4" fill="#ffffff"/>
    
    <!-- Analytics icon -->
    <g transform="translate(800, 200)">
      <rect x="0" y="60" width="20" height="40" fill="#3b82f6"/>
      <rect x="30" y="40" width="20" height="60" fill="#3b82f6"/>
      <rect x="60" y="20" width="20" height="80" fill="#3b82f6"/>
      <rect x="90" y="50" width="20" height="50" fill="#3b82f6"/>
    </g>
    
    <!-- E-commerce icon -->
    <g transform="translate(1200, 180)">
      <rect x="0" y="20" width="100" height="80" rx="8" fill="#3b82f6" stroke="#1e40af" stroke-width="2"/>
      <circle cx="30" cy="60" r="8" fill="#ffffff"/>
      <rect x="50" y="45" width="40" height="8" rx="4" fill="#ffffff"/>
      <rect x="50" y="65" width="30" height="6" rx="3" fill="#ffffff"/>
    </g>
  </g>
  
  <!-- Geometric shapes -->
  <circle cx="1600" cy="300" r="100" fill="rgba(59,130,246,0.05)" opacity="0.8"/>
  <polygon points="300,800 400,900 200,900" fill="rgba(59,130,246,0.04)" opacity="0.6"/>
  <rect x="1400" y="700" width="80" height="80" rx="10" fill="rgba(59,130,246,0.06)" opacity="0.5" transform="rotate(25 1440 740)"/>
</svg>